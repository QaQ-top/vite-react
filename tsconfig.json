{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@src/*": ["./src/*"],
      "@pages/*": ["./src/pages/*"],
      "@route/*": ["./src/route/*"],
      "@assets/*": ["./src/assets/*"],
      "@models/*": ["./src/assets/*"],
      "@themes/*": ["./src/assets/*.less","./src/assets/*.css","./src/assets/*"],
      "@utils/*": ["./src/assets/*"],
    }
  },
  "include": ["./"],
}
