{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext", "ES2015"],
    "types": ["vite/client"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@src/*": ["./src/*"],
      "@pages/*": ["./src/pages/*"],
      "@route/*": ["./src/route/*"],
      "@assets/*": ["./src/assets/*"],
      "@models/*": ["./src/models/*"],
      "@themes/*": ["./src/themes/*.less","./src/themes/*.css","./src/themes/*"],
      "@utils/*": ["./src/utils/*"],
    }
  },
  "exclude": ["./node_modules"],
  "include": ["./src", "./global.d.ts"],
}
